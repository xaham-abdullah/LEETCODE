#include<stdio.h>
// Two Sum

int* twoSum(int* nums, int numsSize, int target, int* returnSize) {
    static int result[2];  // static so we can safely return it
    for (int i = 0; i < numsSize; i++) {
        for (int j = i + 1; j < numsSize; j++) {
            if (nums[i] + nums[j] == target) {
                result[0] = i;
                result[1] = j;
                *returnSize = 2;
                return result;
            }
        }
    }
    *returnSize = 0;
    return NULL;
}Q1Q1
